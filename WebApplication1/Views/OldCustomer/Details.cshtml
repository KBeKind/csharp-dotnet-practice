
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


<h3>@ViewData["Message"]</h3>


<p>There are @ViewData["CustomerCount"] customers in the list</p>

<ul>
    @foreach (var item in ViewBag.Customers)
    {
        <li>@item.Id - @item.Name - @item.Amount</li>
    }

</ul>
<ul>
    @foreach (var item in ViewData["CustomerList"] as List<Customer>)
    {
        <li>@item.Id - @item.Name - @item.Amount</li>
    }

</ul>